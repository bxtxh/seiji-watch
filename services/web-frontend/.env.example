# Example environment variables for web-frontend
# Copy this file to .env.development for local development

# API Gateway URL (default port 8080)
NEXT_PUBLIC_API_BASE_URL=http://localhost:8080

# Site configuration
NEXT_PUBLIC_SITE_NAME=Diet Issue Tracker
NEXT_PUBLIC_SITE_DESCRIPTION=国会の課題をトラッキングするプラットフォーム

# Feature flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=true